if (window.Sentry) {
                Sentry.configureScope(function(scope) {
                    if (window.yaCounter18493072) {
                        scope.setExtra("yandex_client_id", yaCounter18493072.getClientID());
                    }
                    scope.setExtra("is_mobile_version", window.app.isMobile);
                });
            }
});</script></body>
</html>
